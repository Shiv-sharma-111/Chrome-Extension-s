{
  "manifest_version": 2,
  "name":"Wikit",
  "version":"1.0",
  "description":"Search Wikipedia on the go!",
  "icons": {
    "128":"chrome128.png",
    "48":"chrome48.png",
    "16":"chrome16.png"
  },
  "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "contextMenus"
  ]
}
